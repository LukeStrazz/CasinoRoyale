<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="640.0" style="-fx-background-color: D9D9D9;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.casinoroyale.Controller.FirebaseControllers.PrimaryController">
   <children>
      <Pane prefHeight="434.0" prefWidth="494.0" BorderPane.alignment="CENTER">
         <children>
            <TextField id="nameTextField" fx:id="userNameTF" alignment="CENTER" layoutX="220.0" layoutY="14.0" prefHeight="30.0" prefWidth="200.0" promptText="User Name" />
            <TextField id="emailTextField" fx:id="emailTF" alignment="CENTER" layoutX="220.0" layoutY="44.0" prefHeight="30.0" prefWidth="200.0" promptText="Email" />
            <TextField id="passwordTextField" fx:id="password1TF" alignment="CENTER" layoutX="220.0" layoutY="74.0" prefHeight="30.0" prefWidth="200.0" promptText="Password" />
            <TextField fx:id="password2TF" alignment="CENTER" layoutX="220.0" layoutY="104.0" prefHeight="30.0" prefWidth="200.0" promptText="Password Confirmation" />
            <Button id="registerButton" layoutX="289.0" layoutY="277.0" mnemonicParsing="false" onAction="#addData" text="Register" />
            <Text layoutX="461.0" layoutY="268.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already have an account?" />
            <Button layoutX="504.0" layoutY="277.0" mnemonicParsing="false" onAction="#signInButtonHandler" text="Sign In" />
            <RadioButton fx:id="terms" layoutX="195.0" layoutY="255.0" mnemonicParsing="false" text="Terms of Service and Privacy Policy" />
            <TextField id="startingBalanceTextField" fx:id="password2TF1" alignment="CENTER" layoutX="220.0" layoutY="164.0" prefHeight="30.0" prefWidth="200.0" promptText="Starting Balance" />
            <TextField id="ageTextField" fx:id="password2TF2" alignment="CENTER" layoutX="220.0" layoutY="134.0" prefHeight="30.0" prefWidth="200.0" promptText="Age" />
         </children>
      </Pane>
   </children>
   <children>
      <Pane prefHeight="54.0" prefWidth="640.0" BorderPane.alignment="CENTER">
         <children>
            <Text layoutX="213.0" layoutY="42.0" strokeType="OUTSIDE" strokeWidth="0.0" text="REGISTIRATION">
               <font>
                  <Font name="Impact" size="36.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </children>
</AnchorPane>

                    </children>
                </GridPane>
                <Button fx:id="spinButton1" layoutX="382.0" layoutY="422.0" mnemonicParsing="false" onAction="#spinButton1Handler" prefHeight="85.0" prefWidth="85.0" text="\$1 SPIN " />
